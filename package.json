{
  "homepage": "http://treytamaki.github.io/videoroom",
  "name": "videoroom",
  "version": "0.1.0",
  "private": true,
  "main": "./server/proxy.js",
  "dependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "http-proxy": "^1.17.0",
    "nodemon": "2.0.3",
    "socket.io": "^2.2.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "cd server && node proxy.js",
    "proxy": "cd server && nodemon proxy.js",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently \"npm run proxy\" \"npm run server\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.0.0",
    "nodemon": "2.0.3"
  }
}
